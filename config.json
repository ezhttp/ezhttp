{
  "version": 1,
  "listen_addr": "0.0.0.0",
  "listen_port": "8080",
  "banner": ["<!--", "Example Banner", "-->"],
  "csp": {
    "connect-src": [
      "'self'",
      "https://fonts.gstatic.com",
      "https://cdn.jsdelivr.net"
    ],
    "script-src": ["'self'", "'nonce-RANDOM'", "https://cdn.jsdelivr.net"],
    "style-src": [
      "'self'",
      "'nonce-RANDOM'",
      "https://fonts.googleapis.com",
      "https://cdn.jsdelivr.net"
    ]
  },
  "rate_limit": {
    "enabled": true,
    "requests_per_minute": 200,
    "burst_size": 20,
    "cleanup_interval": "10m"
  },
  "tls": {
    "cert_file": "",
    "key_file": ""
  }
}
